	bindcode 46 exec --no-startup-id "xdotool mousemove_relative -- 5 0"

	bindcode Control+44 exec --no-startup-id "xdotool mousemove_relative -- 0 20"

	bindcode Control+45 exec --no-startup-id "xdotool mousemove_relative -- 0 -20"

	bindcode Control+43 exec --no-startup-id "xdotool mousemove_relative -- -20 0"

	bindcode Control+46 exec --no-startup-id "xdotool mousemove_relative -- 20 0"

	bindcode Shift+44 exec --no-startup-id "xdotool mousemove_relative -- 0 1"

	bindcode Shift+45 exec --no-startup-id "xdotool mousemove_relative -- 0 -1"

	bindcode Shift+43 exec --no-startup-id "xdotool mousemove_relative -- -1 0"

	bindcode Shift+46 exec --no-startup-id "xdotool mousemove_relative -- 1 0"

	bindcode 38 exec --no-startup-id "xdotool click 1"

