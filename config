# resize window (you can also use the mouse for that)
mode "resize" {
        # These bindings trigger as soon as you enter the resize mode

        # Pressing left will shrink the window’s width.
        # Pressing right will grow the window’s width.
        # Pressing up will shrink the window’s height.
        # Pressing down will grow the window’s height.
        bindcode 43 resize grow width 1 px or 1 ppt
        bindcode 44 resize grow height 1 px or 1 ppt
        bindcode 45 resize shrink height 1 px or 1 ppt
        bindcode 46 resize shrink width 1 px or 1 ppt

        # same bindings, but for the arrow keys
        bindsym Left resize shrink width 10 px or 10 ppt
        bindsym Down resize grow height 10 px or 10 ppt
        bindsym Up resize shrink height 10 px or 10 ppt
        bindsym Right resize grow width 10 px or 10 ppt

        # back to normal: Enter or Escape or $mod+r
        bindcode 36 mode "default"
        bindcode 9 mode "default"
				bindcode 94 mode "default"
}

bindcode $mod+47 mode "resize"

# Start i3bar to display a workspace bar (plus the system information i3status

bar {
	i3bar_command 		i3bar -t
	status_command		i3blocks
	font							pango:FontAwesome 8
#	font pango:Font Awesome 5 Brands Regular, Font Awesome 5 Brands Regular,Font Awesome 5 Free Solid 
	mode							hide	
	tray_output				none	
	position					top
	workspace_buttons	yes
#	output						primary
	colors {
		background 				#000000 # 90
		statusline			 	#ffff00
		separator 				#ffffff

		focused_workspace	#ffffff #8000a0
		active_workspace	#ffffff #333333
		inactive_workspace	#888888 #000000
		urgent_workspace	#ffffff #900000
	}
}


